<h1>All Posts</h1>
<% Post.all.reverse_each do |post| %>
  <% cache post do %>
  <%= render partial: "shared/post_display", locals: {post: post} %>
  <% if post.auto_book == "auto_book" %>
    <%= link_to "Book Now", '/posts/' + post.id.to_s, class: "btn btn-xs btn-primary" %>
    <br>
  <% else %>
    <%= link_to "Request Reservation", '/posts/' + post.id.to_s, class: "btn btn-xs btn-primary" %><br>
  <% end %>
  <% end %>
<% end %>